{
  "name": "vue3-makestyles",
  "version": "1.1.1",
  "description": "Allows you to write CSS in the form of writing js object, Applicable to vue3",
  "main": "dist/vue-makestyles.cjs.js",
  "jsnext:main": "dist/vue-makestyles.esm.js",
  "module": "dist/vue-makestyles.esm.js",
  "browser": "dist/vue-makestyles.min.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "npm run build:lib && npm run build:dist",
    "build:lib": "babel --out-dir lib src",
    "test": "jest --config jestconfig.json",
    "build:dist": "rollup -c && rollup -c --environment PRODUCTION",
    "build:dev": "rollup -c && rollup -c --environment development",
    "format": "prettier --write \"src/**/*.ts\" \"src/**/*.js\"",
    "lint": "tslint -p tsconfig.json",
    "build:types": "npx tsc"
  },
  "keywords": [
    "vue",
    "vue3",
    "stylis",
    "css",
    "css-in-js",
    "vue-css-in-js",
    "styled",
    "styled-components",
    "vue-styled-components"
  ],
  "files": [
    "/dist"
  ],
  "bugs": {
    "url": "https://github.com/codersmoixan/vue3-makestyles/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/codersmoixan/vue3-makestyles"
  },
  "author": "smoixan",
  "license": "MIT",
  "devDependencies": {
    "@babel/cli": "^7.8.4",
    "@babel/core": "^7.9.0",
    "@babel/plugin-external-helpers": "^7.8.3",
    "@babel/plugin-proposal-class-properties": "^7.18.6",
    "@babel/plugin-proposal-object-rest-spread": "^7.18.9",
    "@babel/preset-env": "^7.19.0",
    "@babel/register": "^7.9.0",
    "@rollup/plugin-typescript": "^8.5.0",
    "@types/jest": "^29.0.0",
    "babel-eslint": "^10.1.0",
    "babel-loader": "^8.1.0",
    "babel-plugin-add-module-exports": "^1.0.4",
    "child_process": "^1.0.2",
    "express": "^4.18.1",
    "jest": "^28.1.3",
    "node-watch": "^0.7.3",
    "path": "^0.12.7",
    "prettier": "^2.7.1",
    "rollup": "^2.79.0",
    "rollup-plugin-babel": "^4.4.0",
    "rollup-plugin-commonjs": "^10.1.0",
    "rollup-plugin-inject": "^3.0.2",
    "rollup-plugin-json": "^4.0.0",
    "rollup-plugin-node-builtins": "^2.1.2",
    "rollup-plugin-node-resolve": "^5.2.0",
    "rollup-plugin-replace": "^2.2.0",
    "rollup-plugin-terser": "^7.0.2",
    "rollup-plugin-visualizer": "^5.8.1",
    "ts-jest": "^28.0.8",
    "tslib": "^2.4.0",
    "tslint": "^6.1.3",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "^4.8.2",
    "vue": "^3.2.39"
  },
  "peerDependencies": {
    "vue": ">=3.2.0"
  },
  "dependencies": {
    "glamor": "^2.20.40",
    "stylis": "3.5.4"
  }
}
